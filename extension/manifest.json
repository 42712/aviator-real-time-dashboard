{
  "manifest_version": 3,
  "name": "Aviator Live Capture",
  "version": "1.0",
  "description": "Captura multiplicadores do Aviator e envia ao painel.",
  "permissions": ["activeTab", "scripting"],
  "host_permissions": ["https://*/*"],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["content.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ]
}
